    <%= form_with(model: profile) do |form| %>
      <% if profile.errors.any? %>
        <div id="error_explanation">
          <h2><%= pluralize(profile.errors.count, "error") %> prohibited this profile from being saved:</h2>

          <ul>
            <% profile.errors.each do |error| %>
              <li><%= error.full_message %></li>
            <% end %>
          </ul> 
        </div>
      <% end %>

      <div class="field mb-3">
        <%= form.text_field :first_name, class: "form-control", placeholder: "first name", autocomplete: "off"%>
      </div>

      <div class="field mb-3">
        <%= form.text_field :last_name, class: "form-control", placeholder: "last name", autocomplete: "off"%>
      </div>

      <div class="field mb-3">
        <%= form.text_field :email, class: "form-control", placeholder: "email", autocomplete: "off" %>
      </div>

      <div class="field mb-3">
        <%= form.label :date_of_birth %>
        <%= form.date_select :date_of_birth, class: "form-control" %>
      </div>

      <div class="field">
        <%= form.number_field :user_id, id: :profile_user_id, type: "hidden" %> 
      </div>
      <%= current_user.id %>

      <div class="actions">
        <%= form.submit "Create", class: "btn btn-primary m-3 ms-0"  %>
      </div>
    <% end %>